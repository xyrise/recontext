{
  "name": "ReconText",
  "version": "1.0",
  "description": "Chrome extension + Svelte boilerplate",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/jquery@3.3.1/dist/jquery.min.js https://cdn.jsdelivr.net/npm/fomantic-ui@2.8.8/dist/semantic.min.js; object-src 'self'",
  "manifest_version": 2
}
